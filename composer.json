{
  "name": "fond-of-spryker/shipments-rest-api",
  "description": "Extend Shipments Rest Api",
  "license": "MIT",
  "authors": [
    {
      "name": "FOND OF"
    }
  ],
  "require": {
    "php": ">=7.2",
    "spryker/shipments-rest-api": "^1.0.0"
  },
  "require-dev": {
    "codeception/codeception": "^2.4",
    "fond-of-codeception/spryker": "dev-master as 1.0.0",
    "php-coveralls/php-coveralls": "^2.1",
    "phpro/grumphp": "^0.14",
    "sebastian/phpcpd": "^4.0",
    "spryker/code-sniffer": "^0.12"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "autoload": {
    "psr-4": {
      "FondOfSpryker\\": "src/FondOfSpryker/"
    }
  },
  "autoload-dev": {
